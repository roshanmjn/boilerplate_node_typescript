{
    "name": "boilerplate_ts",
    "version": "1.0.0",
    "description": "",
    "main": "src/app.ts",
    "scripts": {
        "start": "tsc && node --trace-warnings dist/app.js",
        "build": "tsc",
        "dev": "tsx watch src/app.ts",
        "dev1": "nodemon",
        "dev2": "nodemon -e ts --watch src .env --exec \"npx tsc && node dist/app.js\"",
        "prod": "nodemon --watch src --exec node NODE_ENV=production --trace-warnings src/app.js",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
        "migrate:undo:to": "npx sequelize-cli db:migrate:undo:all --to",
        "seed": "npx sequelize-cli db:seed",
        "seed:undo": "npx sequelize-cli db:seed:undo",
        "seed:all": "npx sequelize-cli db:seed:all",
        "migration:create": "npx sequelize-cli migration:create --name",
        "model:create": "npx sequelize-cli model:create --name",
        "seed:create": "npx sequelize-cli seed:create --name"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.6.7",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "http-status": "^1.7.3",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "moment-timezone": "^0.5.45",
        "morgan": "^1.10.0",
        "mysql2": "^3.6.5",
        "sequelize": "^6.35.2",
        "sequelize-cli": "^6.6.2",
        "socket.io": "^4.7.2",
        "uuid": "^9.0.1",
        "uuidv4": "^6.2.13",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.1.0",
        "@types/sequelize": "^4.28.20",
        "@types/uuid": "^10.0.0",
        "nodemon": "^3.0.2",
        "tslib": "^2.6.3",
        "tsx": "^4.17.0",
        "typescript": "^5.5.4"
    }
}
